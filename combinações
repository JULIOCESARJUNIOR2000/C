#include <stdio.h>
#include <math.h>
int main()
{
    char a,b,c;
    printf("Digite tres letras:");
    scanf("%c%c%c",&a,&b,&c);
    printf("\n-Combinacao 1 = %c%c%c",a,b,c);
    printf("\n-Combinacao 2 = %c%c%c",a,c,b);
    printf("\n-Combinacao 3 = %c%c%c",b,a,c);
    printf("\n-Combinacao 4 = %c%c%c",b,c,a);
    printf("\n-Combinacao 5 = %c%c%c",c,a,b);
    printf("\n-Combinacao 6 = %c%c%c",c,b,a);
    return 0;
}
